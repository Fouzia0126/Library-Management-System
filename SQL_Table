CREATE TABLE users (id INT AUTO_INCREMENT PRIMARY KEY, name VARCHAR(100));
CREATE TABLE books (id INT AUTO_INCREMENT PRIMARY KEY, title VARCHAR(100), author VARCHAR(100));
CREATE TABLE borrows (id INT AUTO_INCREMENT PRIMARY KEY, user_id INT, book_id INT, borrow_date DATE, return_date DATE);
CREATE TABLE fines (id INT AUTO_INCREMENT PRIMARY KEY, borrow_id INT, amount INT);
CREATE TABLE admins (id INT AUTO_INCREMENT PRIMARY KEY, username VARCHAR(50), password VARCHAR(50));
